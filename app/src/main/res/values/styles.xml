<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <!--Top Button insc/conn -->
    <style name="Widget.AppCompat.Button.selector">
        <!--Customize theme here -->
        <item name="android:color">@color/menuWhite</item>

    </style>

    <style name="infofill">

    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <style name="AudioJamTheme" parent="AppTheme" >
        <item name="colorPrimary">@color/menu_top_backgd_green</item>
        <item name="colorPrimaryDark">@color/menu_top_backgd_green</item>
        <item name="colorAccent">@color/menu_top_backgd_green</item>
    </style>

    <style name="Tabstyle" parent="Widget.Design.TabLayout">
        <item name="tabIndicatorColor">@color/white</item>
        <item name="tabBackground">@color/menu_top_backgd_green</item>
        <item name="tabTextAppearance">@style/AppTabTextAppearance</item>
        <item name="tabSelectedTextColor">@color/white</item>
        <item name="android:background">@color/menu_top_backgd_green</item>

    </style>

    <style name="AppTabTextAppearance">
        <item name="android:textColor">@color/textemphasis4</item>
        <item name="textAllCaps">true</item>

    </style>
    <style name="MenuDesignNavigation">
        <item name="colorPrimary"> @color/white</item>
    </style>

</resources>